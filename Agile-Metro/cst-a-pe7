  if evpn-route-type is 1 then
    set extcommunity color c1317
  endif
end-policy
!
route-policy c7001
  if evpn-route-type is 1 or evpn-route-type is 3 then
    set extcommunity color c7001
  endif
end-policy
!
route-policy c7801
  if evpn-route-type is 1 or evpn-route-type is 3 then
    set extcommunity color c7801
  endif
end-policy
!
route-policy ODN-MVPN
  set extcommunity color MLDP additive
  pass
end-policy
!
route-policy ODN_EVPN
  if rd in (101.0.2.52:2001) then
    set extcommunity color c7801
  else
    set extcommunity color BLUE
  endif
  if extcommunity rt in deny_v6_evpn_rt_extcomm then
    drop
  else
    pass
  endif
end-policy
!
route-policy pass-all
  pass
end-policy
!
route-policy set-orange
  set extcommunity color ORANGE
end-policy
!
route-policy DENY-V4-SERV
  if rd in (101.0.1.50:1317, 101.0.2.52:1317) then
    drop
  else
    pass
  endif
end-policy
!
route-policy set-sr-index
  set label-index 252
  pass
end-policy
!
route-policy evpn-color-100
  if evpn-route-type is 1 then
    set extcommunity color color100
  elseif evpn-route-type is 3 then
    set extcommunity color color100
  endif
  pass
end-policy
!
route-policy evpn-color-200
  if evpn-route-type is 1 then
    set extcommunity color color200
  elseif evpn-route-type is 3 then
    set extcommunity color color200
  endif
  pass
end-policy
!
