  if rd in (101.0.2.52:1317) then
    drop
  else
    pass
  endif
end-policy
!
route-policy ADD-PATH-INSTALL
  set path-selection backup 1 install multipath-protect
end-policy
!
route-policy srv6-routes-only
  if community in srmpls then
    drop
  elseif rd in (101.0.2.52:1317, 101.0.2.52:37, 101.0.2.52:5, 101.0.2.52:2001, 100:100, 101.0.2.52:2300, 101.0.2.52:3, 64750:1111) then
    drop
  elseif extcommunity rt in deny_v4_vpnv4_rt_to_v6_rr then
    drop
  else
    pass
  endif
end-policy
!
route-policy color-sdwan-l3vpn
  set community srmpls additive
  set extcommunity color color400
  pass
end-policy
!
route-policy sr-p2mp-core-tree
  set core-tree sr-p2mp
end-policy
!
route-policy SET-L3VPN-ODN-IPv4
  if destination in SR-ODN1-IPv4 then
    set extcommunity color BLUE
  else
    pass
  endif
  if rd in (100:6000, 100:6001, 100:6010, 100:6011, 100:6200, 100:6128) then
    drop
  else
    pass
  endif
end-policy
!
