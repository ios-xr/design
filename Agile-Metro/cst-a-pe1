extcommunity-set opaque BLUE
  100
end-set
!
extcommunity-set opaque c2001
  2001
end-set
!
extcommunity-set opaque c2002
  2002
end-set
!
extcommunity-set opaque color100
  10001
end-set
!
extcommunity-set opaque color200
  10002
end-set
!
extcommunity-set opaque color400
  400
end-set
!
extcommunity-set opaque color800
  800
end-set
!
prefix-set SR-ODN1-IPv4
  123.1.1.0/24
end-set
!
community-set srmpls
  100:5050
end-set
!
route-policy pass
  pass
end-policy
!
route-policy pass-all
  pass
end-policy
!
route-policy ODN_EVPN_SRV6
  if rd in (101.0.1.50:4500, 101.0.1.50:4600, 101.0.1.50:4550, 101.0.1.50:1900) then
    drop
  elseif rd in (101.0.1.50:400) then
    pass
  elseif rd in (101.0.1.50:2001) and evpn-route-type is 1 then
    set extcommunity color c2001
  elseif rd in (101.0.1.50:2001) and evpn-route-type is 3 then
    set extcommunity color c2002
  else
    set extcommunity color BLUE
    pass
  endif
end-policy
!
route-policy evpn-color-100
  if evpn-route-type is 1 then
    set extcommunity color color100
  elseif evpn-route-type is 3 then
    set extcommunity color color100
  endif
  pass
end-policy
!
route-policy evpn-color-200
  if evpn-route-type is 1 then
    set extcommunity color color200
  elseif evpn-route-type is 3 then
    set extcommunity color color200
  endif
  pass
end-policy
!
route-policy SET-L3VPN-ODN-IPv4
  if destination in SR-ODN1-IPv4 then
    set extcommunity color BLUE
  elseif rd in (100:6200, 100:14551) then
    drop
  else
    pass
  endif
end-policy
!
route-policy color-sdwan-l3vpn-800
  set community srmpls additive
  set extcommunity color color800
end-policy
!
route-policy accedian-mgmt-color-400
  if evpn-route-type is 1 then
    set community srmpls additive
    set extcommunity color color400
  endif
  if evpn-route-type is 3 then
    set community srmpls additive
    set extcommunity color color400
  endif
  pass
end-policy
!
