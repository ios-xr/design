!
extcommunity-set rt deny-srv6-services
  100:8000
end-set
!
extcommunity-set opaque BLUE
  100
end-set
!
extcommunity-set opaque c2001
  2001
end-set
!
extcommunity-set opaque c2002
  2002
end-set
!
prefix-set SR-ODN1-IPv4
  101.3.1.0/30,
  100.3.1.0/30,
  122.1.1.0/24
end-set
!
community-set srmpls
  100:5050
end-set
!
route-policy ODN_EVPN_SRV6
  if rd in (101.0.1.54:4540, 101.0.1.54:4600, 101.0.1.54:4554) then
    drop
  elseif rd in (101.0.1.54:400) then
    pass
  elseif rd in (101.0.1.54:2001) and evpn-route-type is 1 then
    set extcommunity color c2001
  elseif rd in (101.0.1.54:2001) and evpn-route-type is 3 then
    set extcommunity color c2002
  else
    set extcommunity color BLUE
    pass
  endif
end-policy
!
route-policy ADD-PATH-INSTALL
  set path-selection backup 1 install multipath-protect
end-policy
!
route-policy srv6-routes-only
  if community in srmpls then
    drop
  elseif rd in (101.0.1.54:1317, 101.0.1.54:1, 100:100, 101.0.1.54:2001) then
    drop
  else
    pass
  endif
end-policy
!
route-policy SET-L3VPN-ODN-IPv4
  if destination in SR-ODN1-IPv4 then
    set extcommunity color BLUE
  elseif rd in (100:6013, 100:14551, 100:6128) then
    drop
  else
    pass
  endif
end-policy
!
router static
 address-family ipv4 unicast
  0.0.0.0/0 172.27.227.5
 !
!
router isis ACCESS
 set-overload-bit on-startup 360
 is-type level-2-only
 net 49.0001.0101.0000.0054.00
 nsr
 distribute link-state instance-id 101
 nsf cisco
 log adjacency changes
 lsp-refresh-interval 65000
 max-lsp-lifetime 65535
 lsp-password keychain ISIS-KEY
 lsp-password keychain ISIS-KEY level 1
 affinity-map red bit-position 0
 address-family ipv4 unicast
  metric-style wide
  mpls traffic-eng level-2-only
  mpls traffic-eng router-id Loopback0
  spf-interval initial-wait 50 secondary-wait 200 maximum-wait 5000
  segment-routing mpls
  spf prefix-priority critical tag 5000
  spf prefix-priority high tag 1000
 !
 address-family ipv6 unicast
  microloop avoidance segment-routing
  router-id Loopback0
  segment-routing srv6
   locator LocAlgo0
   !
   locator LocAlgo128
   !
   locator LocAlgo130
   !
   locator LocAlgo131
   !
  !
 !
 flex-algo 128
  metric-type delay
  advertise-definition
  prefix-metric
 !
 flex-algo 130
  metric-type delay
  advertise-definition
 !
 flex-algo 131
  advertise-definition
  affinity exclude-any red
 !
 interface Loopback0
  address-family ipv4 unicast
   prefix-sid index 54
   prefix-sid algorithm 129 absolute 19054
   prefix-sid algorithm 130 absolute 20054
   prefix-sid algorithm 131 absolute 21054
  !
  address-family ipv6 unicast
  !
 !
 interface HundredGigE0/0/0/0
  bfd minimum-interval 150
  bfd multiplier 3
  bfd fast-detect ipv4
  bfd fast-detect ipv6
  point-to-point
  hello-password keychain ISIS-KEY
  address-family ipv4 unicast
   fast-reroute per-prefix
   fast-reroute per-prefix ti-lfa
   metric 100
  !
  address-family ipv6 unicast
   fast-reroute per-prefix
   fast-reroute per-prefix ti-lfa
   metric 100
  !
 !
