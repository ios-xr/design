router bgp 100
 nsr
 bgp router-id 101.0.1.54
 bgp graceful-restart
 nexthop validation color-extcomm sr-policy
 bgp bestpath aigp ignore
 nexthop validation color-extcomm disable
 ibgp policy out enforce-modifications
 bgp multipath as-path ignore onwards
 address-family ipv4 unicast
  segment-routing srv6
   alloc mode route-policy set_per_prefixv4_locator_rpl
  !
  network 101.0.1.54/32
  redistribute connected
 !
 address-family vpnv4 unicast
  additional-paths receive
  additional-paths send
  additional-paths selection route-policy ADD-PATH-INSTALL
  nexthop trigger-delay critical 0
 !
 address-family ipv6 unicast
  segment-routing srv6
  !
  redistribute connected
 !
 address-family vpnv6 unicast
  additional-paths selection route-policy ADD-PATH-INSTALL
 !
 address-family l2vpn evpn
  nexthop trigger-delay critical 0
 !
 neighbor-group SvRR
  remote-as 100
  update-source Loopback0
  address-family vpnv4 unicast
   route-policy SET-L3VPN-ODN-IPv4 out
   soft-reconfiguration inbound always
  !
  address-family ipv4 mvpn
  !
  address-family ipv6 mvpn
  !
  address-family l2vpn evpn
   route-policy ODN_EVPN_SRV6 out
  !
 !
 neighbor-group SvRR-Client-srv6
  remote-as 100
  update-source Loopback0
  address-family ipv4 unicast
   encapsulation-type srv6
  !
  address-family vpnv4 unicast
   route-policy srv6-routes-only out
  !
  address-family ipv6 unicast
   encapsulation-type srv6
  !
  address-family vpnv6 unicast
  !
  address-family l2vpn evpn
   route-policy srv6-routes-only out
  !
 !
 neighbor fccc:0:10a::1
  remote-as 100
  use neighbor-group SvRR-Client-srv6
  update-source Loopback0
 !
