route-policy evpn-color-200
  if evpn-route-type is 1 then
    set extcommunity color color200
  elseif evpn-route-type is 3 then
    set extcommunity color color200
  endif
  pass
end-policy
!
route-policy ADD-PATH-INSTALL
  set path-selection backup 1 install multipath-protect
end-policy
!
route-policy deny-srv6-services
  if extcommunity rt in deny-srv6-services then
    drop
  else
    pass
  endif
end-policy
!
route-policy deny-sr-mpls-services
  if extcommunity rt in deny-sr-mpls-services then
    drop
  elseif rd in (8000:8500, 8000:8600) then
    drop
  else
    pass
  endif
end-policy
!
router static
 address-family ipv4 unicast
  0.0.0.0/0 172.27.227.5
 !
!
