 neighbor-group SvRR
  remote-as 100
  update-source Loopback0
  address-family vpnv4 unicast
   soft-reconfiguration inbound always
  !
  address-family ipv4 mvpn
  !
  address-family ipv6 mvpn
  !
  address-family l2vpn evpn
   route-policy deny-srv6-services out
  !
 !
 neighbor-group SvRR-Client-srv6
  remote-as 100
  update-source Loopback0
  address-family ipv4 unicast
   encapsulation-type srv6
  !
  address-family vpnv4 unicast
   route-policy deny-sr-mpls-services out
  !
  address-family ipv6 unicast
   encapsulation-type srv6
  !
  address-family vpnv6 unicast
   route-policy deny-sr-mpls-services out
  !
  address-family l2vpn evpn
   route-policy deny-sr-mpls-services out
  !
 !
 neighbor 101.0.1.201
  use neighbor-group SvRR
  shutdown
  description SvRR1-XRV9K
 !
 neighbor 101.0.5.101
  use neighbor-group SvRR
  description *** TO XRD-VRR ***
 !
 neighbor fccc:0:10a::1
  remote-as 100
  use neighbor-group SvRR-Client-srv6
  update-source Loopback0
  address-family ipv4 unicast
   encapsulation-type srv6
  !
  address-family ipv6 unicast
   encapsulation-type srv6
  !
 !
